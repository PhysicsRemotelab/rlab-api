{
  "name": "rlab-api",
  "version": "0.0.1",
  "description": "Remote Control Laboratory for Measurements in Physics",
  "author": "Toomas Tahves",
  "license": "GPLV3",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:docker": "docker-compose --env-file .env.docker up rlab-api",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "jwks-rsa": "^2.0.5",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sequelize": "^6.9.0",
    "sequelize-typescript": "^2.1.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@types/node": "^16.11.6",
    "@types/sequelize": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript-migration": "0.0.1-beta.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
